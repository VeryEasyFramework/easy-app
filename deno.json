{
  "name": "@vef/easy-app",
  "version": "0.3.13",
  "exports": "./mod.ts",
  "exclude": [
    "references"
  ],
  "publish": {
    "exclude": [
      ".github",
      "references",
      "dev"
    ],
    "include": [
      "./**/*",
      "src/client/**/*"
    ]
  },
  "imports": {
    "#/": "./src/",
    "@vef/easy-cli": "jsr:@vef/easy-cli@^0.2.4",
    "@vef/easy-ops": "jsr:@vef/easy-ops@^0.1.3",
    "@vef/easy-orm": "jsr:@vef/easy-orm@^0.3.1",
    "@vef/string-utils": "jsr:@vef/string-utils@^0.1.5"
  },
  "tasks": {
    "dev": "cd dev && deno run -A --unstable-kv main.ts",
    "check": "deno publish --dry-run",
    "update": "deno add @vef/easy-orm @vef/easy-cli @vef/string-utils @vef/easy-ops"
  }
}
