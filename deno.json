{
  "name": "@vef/easy-app",
  "version": "0.2.18",
  "exports": "./mod.ts",
  "exclude": [
    "references"
  ],
  "publish": {
    "exclude": [
      ".github",
      "references",
      "dev"
    ],
    "include": [
      "./**/*",
      "src/client/**/*"
    ]
  },
  "imports": {
    "#/": "./src/",
    "@vef/easy-cli": "jsr:@vef/easy-cli@^0.1.16",
    "@vef/easy-orm": "jsr:@vef/easy-orm@^0.1.24",
    "@vef/string-utils": "jsr:@vef/string-utils"
  },
  "tasks": {
    "dev": "cd dev && deno run -A main.ts",
    "cli": "cd dev && deno run -A main.ts cli",
    "check": "deno publish --dry-run"
  }
}
